            "https://127.0.0.1:8000/jobs/category/:slug".replace(':slug', dataSlug) :
            defaultAction;
        form.attr("action", actionUrl);
    }

    // Initialize form action on page load
    updateFormAction("");

    // Update selected radio value when radio button changes
